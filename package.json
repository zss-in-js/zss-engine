{
  "name": "zss-engine",
  "version": "0.2.6",
  "description": "Zero-runtime Style Sheet Engine",
  "keywords": [
    "zero-runtime",
    "style-sheet",
    "runtime",
    "css-in-js"
  ],
  "author": "Refirst",
  "repository": "github:zss-in-js/zss-engine",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm build:mjs && pnpm cjs",
    "build:mjs": "pnpm esm && for file in $(find dist -name '*.js'); do mv \"$file\" \"${file%.*}.mjs\"; done",
    "cjs": "npx tsc --project tsconfig.cjs.json",
    "esm": "npx tsc --project tsconfig.esm.json",
    "test": "jest"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.6.3"
  }
}
